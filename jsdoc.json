{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            "./src/index.js",      "./src/utils.js",      "./src/constants.js",
            "./src/radix-node.js", "./src/radix-tree.js", "./src/radix-node-edges.js"
        ],
        "includePattern": ".*js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "search": true,
        "static": true,
        "typedefs": true,
        "sort": false
    }
}
